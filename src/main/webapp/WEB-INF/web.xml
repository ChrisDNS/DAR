<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0" metadata-complete="true">
	<display-name>Parisup'</display-name>

	<servlet>
		<servlet-name>Init</servlet-name>
		<servlet-class>com.upmc.parisup.servlets.Init</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>Homepage</servlet-name>
		<servlet-class>com.upmc.parisup.servlets.Homepage</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>com.upmc.parisup.servlets.Login</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SignUp</servlet-name>
		<servlet-class>com.upmc.parisup.servlets.SignUp</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PasswordReset</servlet-name>
		<servlet-class>com.upmc.parisup.servlets.PasswordReset</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Search</servlet-name>
		<servlet-class>com.upmc.parisup.servlets.Search</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Account</servlet-name>
		<servlet-class>com.upmc.parisup.servlets.Account</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LeaveComment</servlet-name>
		<servlet-class>com.upmc.parisup.servlets.LeaveComment</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Homepage</servlet-name>
		<url-pattern></url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SignUp</servlet-name>
		<url-pattern>/signup</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PasswordReset</servlet-name>
		<url-pattern>/password_reset</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Search</servlet-name>
		<url-pattern>/search</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Account</servlet-name>
		<url-pattern>/account</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LeaveComment</servlet-name>
		<url-pattern>/leave_comment</url-pattern>
	</servlet-mapping>


	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>