<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>Homepage</servlet-name>
		<servlet-class>com.upmc.parisup.servlets.Homepage</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>com.upmc.parisup.servlets.Login</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SignUp</servlet-name>
		<servlet-class>com.upmc.parisup.servlets.SignUp</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PasswordReset</servlet-name>
		<servlet-class>com.upmc.parisup.servlets.PasswordReset</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Homepage</servlet-name>
		<url-pattern>/homepage</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SignUp</servlet-name>
		<url-pattern>/signup</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PasswordReset</servlet-name>
		<url-pattern>/password_reset</url-pattern>
	</servlet-mapping>
</web-app>